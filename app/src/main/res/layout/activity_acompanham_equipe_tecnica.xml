<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CadastroBaixaRenda">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Acompanhamento de Manutenção da Equipe Técnica"/>
        <Button
            android:id="@+id/btnPesquisarEquipe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Acompanhar Equipe"/>
        <EditText
            android:id="@+id/etNumRequerimento"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="Digite o número do seu requerimento"
            android:inputType="none|number"/>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
